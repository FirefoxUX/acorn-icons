  "dependencies": {
    "prettier": "^3.6.2",
    "simple-git": "^3.28.0",
    "fast-glob": "^3.3.3",
    "svgo": "^4.0.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@types/node": "^24.7.2",
    "copyfiles": "^2.4.1",
    "typescript": "5.9.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.0"
  }
}
